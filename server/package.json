{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'chore_tracker/*'\" \"npm run chore_tracker\"",
    "chore_tracker": "cd .. && npm run start",
    "install": "cd .. && npm install",
    "build": "cd .. && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.16.3",
    "if-env": "^1.0.4"
  }
}
